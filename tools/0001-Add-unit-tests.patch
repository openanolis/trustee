+                map.insert("key2".to_string(), serde_json::Value::String("value2".to_string()));
+                Ok(map)
+            }),
+            
+            // 无值参数
+            (b"key1 key2=value2", {
+                let mut map = serde_json::Map::new();
+                map.insert("key1".to_string(), serde_json::Value::String("".to_string()));
