+        fn mock_check_report_data(expected_report_data: &[u8], quote_data: &[u8]) -> Result<()> {
+            // 如果expected_report_data或quote_data大于TPM_REPORT_DATA_SIZE，截断至TPM_REPORT_DATA_SIZE
